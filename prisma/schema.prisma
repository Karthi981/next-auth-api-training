generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(uuid())
  acceptTerms Boolean
  email       String   @unique
  password    String
  fullName    String   @unique
  phoneNumber String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Customer {
  id          Int       @id @default(autoincrement())
  name        String
  email       String    @unique
  description String?
  userName    String    @unique
  password    String
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @updatedAt
}

model AuthData {
  id       String @id @default(uuid())
  authData Json?
}

model newTable {
  id          String  @id @default(uuid())
  acceptTerms Boolean
  email       String  @unique
  password    String
  userName    String
  phoneNumber String
}
